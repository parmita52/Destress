<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hshack.destress">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Breather"
            android:label="@string/title_activity_breather" />
        <activity
            android:name=".BreatherFocus1"
            android:hardwareAccelerated="false"
            android:label="@string/title_activity_breather_focus1" />
        <activity
            android:name=".BreatherFocus2"
            android:label="@string/title_activity_breather_focus2" />
        <activity
            android:name=".BreatherFocus3"
            android:label="@string/title_activity_breather_focus3" />
        <activity
            android:name=".BreatherFocus4"
            android:label="@string/title_activity_breather_focus4" />
        <activity
            android:name=".BreatherFocus5"
            android:label="@string/title_activity_breather_focus5" />
        <activity
            android:name=".BreatherFocus6"
            android:label="@string/title_activity_breather_focus6" />
        <activity
            android:name=".Notifications"
            android:label="@string/title_activity_notifications" />
        <activity
            android:name=".Messages"
            android:label="@string/title_activity_messages" />
        <activity
            android:name=".Sounds"
            android:label="@string/title_activity_sounds" />
        <activity android:name=".Feelings"></activity>
    </application>

</manifest>